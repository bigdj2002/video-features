{
    "configurations": [
        {
            "name": "(gdb) feature unit tester",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../build/apps/feature_unit_tester/feature_unit_tester",
            "args": [
                "--gtest_print_time", "1"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) feature generator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../build/apps/feature_generator/feature_generator",
            "args": [
                // "-i", "/source/jvet-test-sequences/ctc/sdr/BasketballDrive_1920x1080_50.yuv",
                "-i", "/mnt/sDisk2/jvet-test-sequences/ctc/sdr/BasketballDrive_1920x1080_50.yuv",
                "-o", "${workspaceFolder}/stat.json",
                "-f", "50",
                "-w", "1920",
                "-h", "1080",
                "-s", "1",
                "-t", "40"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}